{
  "name": "eslint-config-tdmalone",
  "version": "0.0.1",
  "description": "My personal shareable ESLint config. Heavily influenced by the WordPress style. Includes ESLint recommended rules. For use in either browser or Node. Support for ES2018, Jest, JSDoc and jQuery.",
  "homepage": "https://github.com/tdmalone/eslint-config-tdmalone#readme",
  "repository": "git+https://github.com/tdmalone/eslint-config-tdmalone.git",
  "main": "index.js",
  "author": "Tim Malone <tdmalone@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "DEBUG=eslint:cli-engine eslint --color --ignore-pattern '/coverage/' --ignore-pattern '!.*.js' \"**/*.js\"",
    "fix": "yarn lint --fix"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "javascript",
    "jest"
  ],
  "dependencies": {
    "eslint": "^5.6.1",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-dollar-sign": "^1.0.1",
    "eslint-plugin-jest": "^21.23.0"
  },
  "devDependencies": {}
}
